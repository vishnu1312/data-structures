#include<stdio.h>
#include<conio.h>
main()
{
	int a[10],i;
	printf("enter the values");
	for(i=0;i<7;i++)
	{
		scanf("%d",&a[i]);
		
	}
	printf("even numbers are:");
	
		for(i=0;i<7;i++)
		{
		if(a[i]%2==0)
		{
			printf("%d\n",a[i]);
		}	
		}
	printf("odd numbers are:");
	for(i=0;i<7;i++)
	{
		if(a[i]%2==1)
		{
			printf("%d\n",a[i]);
		}
}
}
